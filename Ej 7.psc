Proceso velocidadObje
	Definir distancia, tiempo, velocidad Como Real;
	Escribir 'Digite la distancia en metros que recorrio el objeto';
	Leer distancia;
	Escribir 'DIgite el tiempo en segundos en el que el obejeto recorrio la distancia';
	Leer tiempo;
	velocidad <- distancia/tiempo;
	Escribir 'La velocidad que tiene el objeto es de ', velocidad, ' m/s';
FinProceso
