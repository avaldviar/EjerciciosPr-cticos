Algoritmo totalComp
	Definir canti Como Entero;
	Definir preci Como Entero;
	Definir total Como Entero;
	Escribir "Digite la cantidad de producto adquirido ";
	Leer canti;
	Escribir "Digite el precio por unidad del producto adquirido ";
	Leer preci;
	total<- canti*preci;
	Escribir "El total a pagar es ", total;
	Escribir lista_de_expresiones;
	
FinAlgoritmo
